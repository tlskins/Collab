    <section class="jumbotron text-center">
      <div class="container">
        <h1 class="jumbotron-heading"><b>Collab: </b><%= @collab_project.name %></h1>
        <p class="lead text-muted"><b>Description: </b><%= @collab_project.description %></p>
        <p class="lead text-muted"><b>Created at: </b><%= @collab_project.created_at.strftime('%-d %B %Y %H') %></p>
        <p>
          <a href=<%= new_collab_project_branch_path(@collab_project) %> class="btn btn-primary">Create Branch</a>
        </p>
      </div>
    </section>

    <section class="jumbotron text-center">
      <div class="container">
        <p class="lead text-muted"><b><%= link_to @branch.collabproject.name + ' collab', collab_project_path(@branch.collabproject)  %></b>
        <% if @branch.parent_branch %>
          <p class="lead text-muted"><b><%= link_to 'Parent Branch: ' + @branch.parent_branch.name, collab_project_branch_path(:collab_project_id => @branch.collabproject.id, :id => @branch.parent_branch.id)  %></b>
        <% end %>
        <h1 class="jumbotron-heading"><b>Branch: </b><%= @branch.name %></h1>
        <p class="lead text-muted"><b>Description: </b><%= @branch.description %></p>
        <p class="lead text-muted"><b>Created at: </b><%= @branch.created_at.strftime('%-d %B %Y %H') %></p>
        <p>
          <a href='#' class="btn btn-primary">Edit Branch Details</a>
          <a href=<%= new_collab_project_branch_leaf_path(@branch.collabproject, @branch) %> class="btn btn-primary">Add Leaf</a>
          <a href=<%= collab_project_branch_new_child_branch_path(@branch.collabproject, @branch) %> class="btn btn-primary">Add Branch</a>
        </p>
      </div>
    </section>

  <div class="container">
    <div id="player-wrapper"></div>
      <div class="text-center row vertical-center-row">
        <h1>Current Leaf</h1>
          <% if @leaf %>
            <div class="text-center col-md-4 col-md-offset-4">
              <div class="yt_video thumbnail">
                <%= link_to image_tag("https://img.youtube.com/vi/#{@leaf.uid}/mqdefault.jpg", alt: @leaf.title,
                                      class: 'img-rounded'),
                            "https://www.youtube.com/watch?v=#{@leaf.uid}", target: '_blank' %>
                <div class="caption">
                  <h5><%= @leaf.title %></h5>
                  <p><b>Published at:</b> <%= @leaf.published_at %></p>
                  <p><b>Link:</b> <%= @leaf.link %></p>
                  <p>
                    <span class="glyphicon glyphicon glyphicon-thumbs-up"></span> <%= @leaf.likes %>
                    <span class="glyphicon glyphicon glyphicon-thumbs-down"></span> <%= @leaf.dislikes %>
                  </p>
                </div>
              </div>
            </div>
          <% end %>
      </div>
  </div>

<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h1 class='page-header'><%= @branch.name %> branch
        <small>branches</small>
      </h1>
    </div>
  </div>

  <% if @child_branches.any? %>
    <% @child_branches.each do |branch| %>
      <%= render branch %>
    <% end %>
  <% else %>
    <h3>No Branches</h3>
  <% end %>
</div>


