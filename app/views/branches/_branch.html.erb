  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href=<%= '#collapse' + branch.id.to_s %>>
        <%= branch.name %></a>
      </h4>
    </div>
    <div id=<%= 'collapse' + branch.id.to_s %> class="panel-collapse collapse in">
      <div class="panel-body">
        <%= branch.purpose %><br />
        <b>Created on: </b><%= branch.created_at.strftime('%-d %B %Y') %><br /><br />
        <a class="btn btn-primary btn-sm" href=<%= collab_project_branch_path(:collab_project_id => branch.collabproject.id, :id => branch.id) %>>Goto Branch<span class="glyphicon glyphicon-chevron-right"></span></a><br /><br />
      </div>
    </div>
  </div>

